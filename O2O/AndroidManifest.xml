<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.m6.o2o"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.m6.o2o.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.m6.o2o.LoginActivity"
            android:label="@string/title_activity_login"
            android:windowSoftInputMode="adjustResize|stateVisible" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.m6.o2o.DeliveryActivity"
            android:label="@string/title_activity_delivery" >
        </activity>
        <activity
            android:name="com.m6.o2o.OvertimeActivity"
            android:label="@string/title_activity_overtime" >
        </activity>
        <activity
            android:name="com.m6.o2o.LogActivity"
            android:label="@string/title_activity_log" >
        </activity>
    </application>

</manifest>